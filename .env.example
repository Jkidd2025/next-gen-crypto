# Solana RPC Configuration
VITE_PRIMARY_RPC_URL=https://api.mainnet-beta.solana.com
VITE_PRIMARY_RPC_API_KEY=your_api_key_here
VITE_BACKUP_RPC_URL=https://solana-api.projectserum.com
VITE_BACKUP_RPC_API_KEY=your_backup_api_key_here
VITE_FALLBACK_RPC_URL=https://rpc.ankr.com/solana
VITE_FALLBACK_RPC_API_KEY=your_fallback_api_key_here

# Rate Limiting Configuration
VITE_RATE_LIMIT_MAX_REQUESTS=100
VITE_RATE_LIMIT_WINDOW_MS=60000
VITE_RATE_LIMIT_BLOCK_DURATION_MS=300000

# Connection Configuration
VITE_CONNECTION_TIMEOUT_MS=30000
VITE_MAX_RETRIES=3
VITE_RETRY_DELAY_MS=1000

# Monitoring Configuration
VITE_ENABLE_MONITORING=true
VITE_LOG_LEVEL=info
VITE_METRICS_ENDPOINT=https://metrics.yourdomain.com